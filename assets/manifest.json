{
  "manifest_version": 3,
  "name": "Booke AI for Xero",
  "version": "1.1",
  "description": "You can perform bulk reconcile for Xero transactions with this extension",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "css": ["main.css"],
      "matches": [
        "https://go.xero.com/*"
      ]
    }
  ]
}